{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "databricks-sdk-py": {
      "command": "npx",
      "args": ["mcp-remote", "https://gitmcp.io/databricks/databricks-sdk-py"]
    },
    "databricks-blog": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/databricks-solutions/databricks-blogposts"
      ]
    },
    "databricks-ctx-eng": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/juanlamadrid20/databricks-context-engineering-cookbook"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "~/dev"]
    },
    "serena": {
      "command": "/opt/homebrew/bin/uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ]
    }
  }
}
